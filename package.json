{
  "name": "helmops",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbo",
    "dev:turbopack": "next dev --turbo",
    "dev:webpack": "NEXT_DISABLE_TURBOPACK=1 next dev",
    "dev:mobile": "next dev --turbo -H 0.0.0.0",
    "build": "NEXT_TURBOPACK=1 prisma generate && next build --turbo",
    "start": "next start",
    "lint": "eslint",
    "typecheck": "tsc --noEmit",
    "analyze": "ANALYZE=true next build --turbo",
    "db:seed": "tsx prisma/seed.ts",
    "db:seed-plans": "tsx prisma/seed-plans.ts",
    "create-super-admin": "tsx scripts/create-super-admin.ts",
    "cleanup-duplicate-channels": "tsx scripts/cleanup-duplicate-channels.ts",
    "create-icons": "./scripts/create-icons.sh",
    "deploy": "./scripts/deploy.sh",
    "postinstall": "prisma generate && (npm run create-icons || echo 'Icon creation skipped (ImageMagick not found)')"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@hookform/resolvers": "^5.2.2",
    "@prisma/client": "^6.19.0",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-switch": "^1.2.6",
    "@sentry/nextjs": "^10.32.1",
    "@supabase/ssr": "^0.8.0",
    "@supabase/supabase-js": "^2.88.0",
    "@types/pg": "^8.15.6",
    "@types/web-push": "^3.6.4",
    "@types/xlsx": "^0.0.35",
    "ai": "^6.0.3",
    "bcryptjs": "^3.0.3",
    "browser-image-compression": "^2.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "framer-motion": "^12.23.26",
    "jsonwebtoken": "^9.0.3",
    "jspdf": "^3.0.4",
    "lucide-react": "^0.555.0",
    "next": "^16.1.1-canary.8",
    "next-auth": "^5.0.0-beta.30",
    "nodemailer": "^7.0.12",
    "openai": "^6.15.0",
    "pg": "^8.16.3",
    "prisma": "^6.19.0",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-hook-form": "^7.67.0",
    "recharts": "^2.15.4",
    "server-only": "^0.0.1",
    "tailwind-merge": "^3.4.0",
    "web-push": "^3.6.7",
    "xlsx": "^0.18.5",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20",
    "@types/nodemailer": "^7.0.4",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "16.0.7",
    "tailwindcss": "^4",
    "tsx": "^4.21.0",
    "typescript": "^5"
  }
}
